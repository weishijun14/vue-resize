    "eslint-plugin-vue": "^7.1.0",
    "fs-extra": "^9.0.1",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
